<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
    {% block stylesheets %}
        {# {{ encore_entry_link_tags('app') }} #}
        <link rel="stylesheet" href="{{ asset('assets/css/boostrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/css/main.css') }}">
    {% endblock %}

</head>
<body>

{% block body %}{% endblock %}

{% block body_javascripts %}
    {# {{ encore_entry_script_tags('app') }} #}
    <script src="{{ asset('assets/js/library/jquery-331-slim.js') }}"></script>
    <script src="{{ asset('assets/js/library/popper.min.js') }}"></script>
    <script src="{{ asset('assets/js/library/bootstrap.min.js') }}"></script>

{% endblock %}

</body>
</html>
